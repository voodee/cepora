.btn 
  border-radius 0

  &-border
    position relative
    background none repeat scroll 0 0 rgba(0, 0, 0, 0)
    width auto
    display inline-block
    border .2rem solid #fff
    color #fff
    transition all 0.3s ease 0s
    z-index 1

    &:after 
      content ''
      height 0
      left 0
      position absolute
      top 0
      transition all 0.3s ease 0s
      width 100%
      z-index -1
      background #fff

      
    &:hover
    &:focus
      color #ec3642
      
      &:after
        height 100%

    &-red
      border-color #ec3642

      &:after 
        background #ec3642
        
      &:hover
      &:focus
        color #fff
        
      &-full
        color #ec3642






