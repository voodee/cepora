.steps
  background #696969
  color #fff
  padding 5rem 0
  
  .section-title
    text-align center
    
    @media (min-width: $screen-md-min)
      text-align left
    
    h3
      font-size 3rem
      margin-top 0

  &-item
    
    p
      position relative
      height 41px
      
      width 60%
      margin 20px auto 20px
      padding 0 15px 0 50px
    
      @media (min-width: $screen-md-min)
        width auto
        margin 0 0 20px 0
        padding 0 0 0 46px

      &:before
        content ''
        position absolute
        top 0
        left 0
        width 41px
        height 41px
        background url('../img/sprite-steps.png') 0 0 no-repeat
      
      &:after
        content ''
        position absolute

        top auto
        right -12px
        bottom 100%
        background url('../img/sprite-icons.png') no-repeat 0 -105px
        height 61px
        width 18px
        margin 0 0 -14px
        
        @media (min-width: $screen-md-min)
          top 100%
          right 100%
          bottom auto
          background url('../img/sprite-icons.png') no-repeat 0 -72px
          width 61px
          height 13px
          margin -1px 15px 0 0
        
        
    &:nth-child(2)
      p
        &:before
          background-position 0 -56px  
        
    &:nth-child(3)
      p
        &:before
          background-position 0 -117px
        
    &:nth-child(4)
      p
        &:before
          background-position 0 -174px
        
    &:nth-child(1)
      p
        &:after
          display none
          
    @media (max-width: $screen-md-min)
      &:nth-child(odd)
        p
          &:after
            right auto
            left -33px
            transform scaleX(-1)
            margin 0 0 -14px
      







